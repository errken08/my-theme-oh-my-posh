{
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#002F52",
          "foreground": "#0092FF",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "\uf508 {{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "#0092FF",
          "foreground": "#002F52",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "folder"
          },
          "style": "powerline",
          "template": " \uf07b {{ .Path }} ",
          "type": "path"
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#193549",
          "background": "#9A5FEB",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "properties": {
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            }
          }
        },
        {
          "background": "#FFDE57",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "powerline",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#8CE10B",
          "background": "#2B4404",
          "template": "  {{ .Full }} "
        },
        {
          "type": "npm",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#FF000F",
          "background": "#323232",
          "template": "  {{ .Full }} "
        },
        {
          "type": "java",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#E34C00",
          "background": "#323232",
          "template": "  {{ .Full }}"
        }    
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
                        {
          "background": "#8CE10B",
          "foreground": "#2B4404",
          "leading_diamond": "\ue0b6",
          "properties": {
            "time_format": "2006-01-02"
          },
          "style": "diamond",
          "template": " \uf133 {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b4",
          "type": "time"
        },
        {
          "background": "#2B4404",
          "foreground": "#8CE10B",
          "leading_diamond": "\ue0b6",
          "properties": {
            "time_format": "15:04 PM"
          },
          "style": "diamond",
          "template": " \uf64f {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b4",
          "type": "time"
        },
        {
          "background": "#272727",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          // "template": " \uf1bb {{ .Env.DAYS }} days till Christmas ",
          "template": "\uF408 errken08",
          "trailing_diamond":"\ue0b4",
          "type": "text"
        }
      ],

      "type": "prompt"
    },
     {
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "diamond",
          "foreground": "#FF000F",
          "template": " \uF0E7 "
        },
        {
          "foreground": "#ffffff",
          "style": "diamond",
          "template": " \uf120 ",
          "type": "text"
        }
      ],

      "type": "prompt"
    }
  ],
  "console_title_template": "PowerShell",
  "final_space": true,
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "<#1f4b8e,transparent>\ue0b6</><#ffffff,#1f4b8e>\uf2dc</><#1f4b8e,transparent>\ue0b4</> {{ .Command }}"
  },
  "version": 2
}
